<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.requerimientos.login.Destinos_Encontrados" >

    <TextView
        android:id="@+id/txtCoincident"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:text="@string/title_destinos"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/txtCoinc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtCoincident"
        android:layout_below="@+id/txtCoinc1"
        android:layout_marginTop="60dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/txtCoinc3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtCoinc2"
        android:layout_centerVertical="true"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/txtCoinc4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtCoinc3"
        android:layout_below="@+id/txtCoinc3"
        android:layout_marginTop="39dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/btnCoinc2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtCoinc2"
        android:layout_alignBottom="@+id/txtCoinc2"
        android:layout_alignRight="@+id/btnCoinc1"
        android:background="@drawable/ic_action_forward"/>

    <Button
        android:id="@+id/btnCoinc3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtCoinc3"
        android:layout_alignBottom="@+id/txtCoinc3"
        android:layout_alignLeft="@+id/btnCoinc2"
        android:background="@drawable/ic_action_forward"/>

    <Button
        android:id="@+id/btnCoinc4"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtCoinc4"
        android:layout_alignBottom="@+id/txtCoinc4"
        android:layout_alignLeft="@+id/btnCoinc3"
        android:background="@drawable/ic_action_forward"/>

    <Button
        android:id="@+id/btnCoinc5"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtCoinc5"
        android:layout_alignBottom="@+id/txtCoinc5"
        android:layout_alignLeft="@+id/btnCoinc4"
        android:background="@drawable/ic_action_forward"/>

    <TextView
        android:id="@+id/txtCoinc5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtCoinc4"
        android:layout_below="@+id/btnCoinc4"
        android:layout_marginTop="46dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/btnCoinc1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtCoinc1"
        android:layout_alignBottom="@+id/txtCoinc1"
        android:layout_alignRight="@+id/txtCoincident"
        android:layout_marginRight="22dp"
        android:background="@drawable/ic_action_forward" />

    <TextView
        android:id="@+id/txtCoinc2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtCoinc1"
        android:layout_below="@+id/btnCoinc1"
        android:layout_marginTop="54dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>